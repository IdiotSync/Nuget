    <PackageReference Include="SereniaBLPLib" Version="1.0.0" />
    <PackageReference Include="runtime.linux-x64.CoreCompat.System.Drawing" Version="1.0.0-beta009" />
  </ItemGroup>

  <ItemGroup>
    <ProjectReference Include="ExternalLib\CascLib\CascLib\CascLib.csproj" />
  </ItemGroup>
</Project>
